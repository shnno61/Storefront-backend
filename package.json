  "main": "server.ts",
  "scripts": {
    "start": " tsc && node dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "tsc": "tsc",
    "dev": " nodemon src/server.ts",
    "tests":"db-migrate db:create shema-test && db-migrate --env test up && set ENV=test jasmine  && db-migrate --env test down && db-migrate db:drop storefront_test",
    "test": "set NODE_ENV=test && tsc && db-migrate up schema-dev --env test && tsc&& jasmine && db-migrate reset --env test ",
    "format": "prettier --write src/**/**/*.ts",
    "lint": "eslint src/**/**/*.ts --fix",
    "db-up": "db-migrate up shema-dev  "

  },
